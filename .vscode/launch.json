{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/src/Web/bin/Debug/netcoreapp1.0/Web.dll",
            "args": [],
            "cwd": "${workspaceRoot}/src/Web",
            "stopAtEntry": false,
            "externalConsole": true,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}